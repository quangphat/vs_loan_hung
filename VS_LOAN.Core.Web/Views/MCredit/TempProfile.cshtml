    function getFilesUpload() {
        $.ajax({
            type: "POST",
            url: '/MCredit/GetFileUpload?profileId=' + @ViewBag.model.Id + '&profileTypeId=3',
            data: {},
            success: function (data) {
                if (data.data != null) {
                    var i = 0;
                    var htmlContent = "";
                    let keys = [];
                    $.each(data.data, function (index, item) {
                        $("#lstTailieu").append("<div class='form-group'><div id='tailieu-" + item.ID + "'></div></div > ");
                        if (!isNullOrNoItem(item.Tailieus)) {

                            let _initialPreview = [];
                            let _initialPreviewConfig = [];
                            $.each(item.Tailieus, function (i, tailieu) {
                                //fileRequireIds.push(item.ID);
                                _initialPreview = [];
                                _initialPreviewConfig = [];
                                var _imgExts = ["jpg", "jpeg", "png", "pdf"];
                                var extn = tailieu.FileUrl.split('.').pop();
                                var isImg = isExtension(extn, _imgExts);
                                if (isImg) {
                                    _initialPreview.push(tailieu.FileUrl);
                                    _initialPreviewConfig.push({
                                        caption: tailieu.FileName,
                                        url: "/hoso/RemoveTailieuByHoso?hosoId=" + item.ProfileId + "&fileId=" + tailieu.FileId,
                                        type: extn.indexOf('pdf') > -1 ? "pdf" : "jpg",
                                        width: "120px"
                                    });
                                }
                                let keyName = '',
                                    isrequire = false,
                                    className = ''
                                
                                if (keys.indexOf(tailieu.Key) < 0) {
                                    keyName = tailieu.KeyName;
                                    isrequire = true
                                }
                                else {
                                    className = 'mt-46'
                                }
                                // renderOneItemFile(tailieu.Key, tailieu.FileId, name, isrequire, className, true, _initialPreview, _initialPreviewConfig, true, true, onUploadFile, onDeleteFile)
                                renderOneItemFile_MCredit(
                                    {
                                        key: tailieu.Key,
                                        fileId: tailieu.FileId,
                                        titleName: keyName,
                                        isRequire: isrequire,
                                        profileId: item.ProfileId,
                                        documentName: item.DocumentName,
                                        documentCode: item.DocumentCode,
                                        documentId: item.DocumentId,
                                        groupId: item.GroupId
                                    }
                                    , className, true, _initialPreview, _initialPreviewConfig, true, true, onUploadFile, onDeleteFile, filesUpload);
                                keys.push(item.ID)
                            })
                            if (item.Tailieus.length < 50) {
                                //renderOneItemFile(item.ID, item.ID, '', false, 'mt-46', true, [], [], true, false, onUploadFile, onDeleteFile)
                                renderOneItemFile_MCredit(
                                    {
                                        key: item.ID,
                                        fileId: 0,
                                        titleName: '',
                                        isRequire: false,
                                        profileId: item.ProfileId,
                                        documentName: item.DocumentName,
                                        documentCode: item.DocumentCode,
                                        documentId: item.DocumentId,
                                        groupId: item.GroupId
                                    }
                                    , 'mt-46', true, [], [], true, false, onUploadFile, onDeleteFile, filesUpload);
                            }

                        }
                        else {
                            renderOneItemFile_MCredit(
                                {
                                    key: item.ID,
                                    fileId: 0,
                                    titleName: item.Ten,
                                    isRequire: item.BatBuoc,
                                    profileId: item.ProfileId,
                                    documentName: item.DocumentName,
                                    documentCode: item.DocumentCode,
                                    documentId: item.DocumentId,
                                    groupId: item.GroupId
                                }
                                , 'mt-46', true, [], [], true, false, onUploadFile, onDeleteFile, filesUpload);
                            //keys.push(item.ID);
                        }
                        //------------
                       
                       
                    })
                }

            },
            complete: function () {
            },
            error: function (jqXHR, exception) {
                showError(jqXHR, exception);
            }
        });
    }
    let filesUpload = [];
    function onDeleteFile(key, fileId, isExist) {
        removeFile(key, fileId);
    }
    function onUploadFile(model, data, isExist) {

        if (isReach5Files(filesUpload, model.key))
            return;
        if (isNullOrNoItem(filesUpload)) {
            filesUpload = []
        }
        let file = {
            Key: parseInt(data.initialPreviewConfig[0].key),
            FileName: data.initialPreviewConfig[0].caption,
            FileUrl: data.initialPreview,
            KeyName: "",
            Id: model.fileId,
            documentId: model.documentId
        }
        let existKeyIndex = filesUpload.findIndex(p => p.key == model.key)
        if (existKeyIndex < 0) {
            filesUpload.push({ key: parseInt(model.key), files: [file], documentId: model.documentId })
        }
        else {
            if (filesUpload[existKeyIndex].files == null)
                filesUpload[existKeyIndex].files = []
            filesUpload[existKeyIndex].files.push(file);
        }
        if (isReach5Files(filesUpload, model.key) === false) {
            model.fileId = getNewGuid();
            model.isRequire = false
            //model.titleName = ''
            renderOneItemFile_MCredit(model, 'mt-46', true, [], [], true, false, onUploadFile, onDeleteFile, filesUpload);
        }
    }
    function removeFile(key, fileId) {
        if (isNullOrNoItem(filesUpload))
            return;
        let index = filesUpload.findIndex(p => p.key == key);
        if (index >= 0) {
            if (!isNullOrNoItem(filesUpload[index].files)) {
                let indexOfFile = filesUpload[index].files.findIndex(p => p.Id == fileId);
                if (indexOfFile >= 0)
                    filesUpload[index].files.splice(indexOfFile, 1);
            }
        }
    }



