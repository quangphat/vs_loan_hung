    <script type="text/javascript">
        function SetActiveMenu(menuId, level) {
            $('#sidebar li .open').removeClass("open");
            $('#sidebar li .active').removeClass("active");
            if (level == 1) {
                $('#sidebar li[id="' + menuId + '"]').addClass('active');
            } else if (level == 2) {
                $('#sidebar li[id="' + menuId + '"]').addClass('active');
                $('#sidebar li[id="' + menuId + '"]').parent().parent().addClass('active open');
            }
            else if (level == 3) {
                $('#sidebar li[id="' + menuId + '"]').addClass('active');
                $('#sidebar li[id="' + menuId + '"]').parent().parent().addClass('active open');
                $('#sidebar li[id="' + menuId + '"]').parent().parent().parent().parent().addClass('active open');
            }
        }
    </script>
