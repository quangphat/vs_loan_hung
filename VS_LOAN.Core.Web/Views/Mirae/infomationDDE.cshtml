function checkDUP( value) {
   
    if(value =="" || value.length >12 || value.length < 9)
    {
            swal({
                    title: "Thành công",
                    text: ` Dữ liệu không hợp lệ:  CMND 9 số, CCCD  là 10 số`,
                    type: "error",
                    timer: 4000,
                    showConfirmButton: true,
                }, function () {

                });
            return;
    }

    $('#panel_body').block();

    if (isNullOrWhiteSpace(value))
        return;
        var objectSend = JSON.stringify({
            'Value': value
        });
        $.ajax({
            traditional: true,
            url: '/mirae/CheckCMND',
            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: objectSend,
            success: function (data) {
                var result =data.data.Data;
                if (result.StatusNumber == "0") {
                    swal({
                        title: "Thành công",
                        text: 'Đủ điều kiện',
                        type: "success",
                        timer: 4000,
                        showConfirmButton: true,
                    }, function () {

                    });
                }
                else {
                    swal({
                        title: "Khách hàng",
                        text: 'Không đủ điều kiện',
                        type: "error",
                        timer: 4000,
                        showConfirmButton: true,
                    });
                }
            },
            error: function (jqXHR, exception) {
                showError(jqXHR, exception);
              
            },
            complete: function () {
                $('#panel_body').unblock();
            },
        });
   

}
</script>