
@{
    ViewData["Title"] = "Check Duplicate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="main-content-inner">
    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="fa fa-caret-right"></i>
                <a href="#">Check Duplicate</a>
            </li>
        </ul>
    </div>
    <div class="page-content" id="panel_body">
        <div class="row">
            <div class="col-xs-12">
                <div class="col-xs-4">
                    <form role="form">
                        <div class="form-group">
                            <label for="form-field-1">Số CMND/CCCD/CMQĐ <span class="required">(*)</span> </label>
                            <input type="text" id="txtValue" placeholder="" class="form-control">
                        </div>
                        <button type="button" id="btnSave" class="btn btn-info pull-right">Kiểm tra</button>
                    </form>
                    <div id="checkresult-area">
                        <div class="col-sm-10">
                            <label style="color:green" id="checkresult"> </label>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script>


    $("#txtValue").ForceNumericOnlyv2();

    $('#btnSave').click(function (e) {
        checkDup('checkresult', $('#txtValue').val())
    });

</script>

