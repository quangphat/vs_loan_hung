       function doLogout() {
        $.ajax({
            type: "POST",
            datatype: "JSON",
            url: '@Url.Action("Logout", "Account")',
            success: function (data) {
                 window.location = "/Account/Login";
            },
            error: function (jqXHR, exception) { if (jqXHR.status != 0) alert('Error'); }
        });
    };
   </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
